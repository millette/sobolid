{
  "name": "sobolid",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "VITE_HASHED_VERSION=`git rev-parse HEAD` vite build",
    "deploy": "VITE_HASHED_VERSION=`git rev-parse HEAD` VITE_PATHPREFIX=/sobolid/ vite build && gh-pages -d dist",
    "serve": "vite preview",
    "format": "prettier --loglevel warn --write \"**/*.{js,css,md,ts,yaml,json,tsx,html}\"",
    "posttest": "npm run format",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=6.0.0"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@unocss/preset-wind": "^0.22.6",
    "@unocss/reset": "^0.22.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-solid": "^0.4.4",
    "fast-glob": "^3.2.11",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "mrm": "^3.0.10",
    "prettier": "^2.5.1",
    "rollup": "^2.66.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "typescript": "^4.5.5",
    "unocss": "^0.22.6",
    "vite": "^2.7.13",
    "vite-plugin-solid": "^2.2.5"
  },
  "dependencies": {
    "@solid-primitives/storage": "^1.1.0",
    "@supabase/supabase-js": "^1.29.4",
    "date-fns": "^2.28.0",
    "solid-app-router": "^0.2.1",
    "solid-js": "^1.3.5",
    "solid-meta": "^0.27.3",
    "solid-supabase": "^0.0.6"
  },
  "lint-staged": {
    "*.{js,css,md,ts,yaml,json,tsx,html}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "follow-redirects": ">=1.14.7",
      "nanoid": ">=3.2.0",
      "node-fetch": ">=2.6.7"
    }
  }
}
