{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "VITE_HASHED_VERSION=`git rev-parse HEAD` vite build",
    "deploy": "VITE_PATHPREFIX=/sobolid/ vite build && gh-pages -d dist",
    "serve": "vite preview",
    "format": "prettier --loglevel warn --write \"**/*.{js,css,md,ts,yaml,json,tsx,html}\"",
    "posttest": "npm run format",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "@unocss/preset-wind": "0.22.3",
    "@unocss/reset": "0.22.3",
    "eslint": "8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "fast-glob": "3.2.11",
    "gh-pages": "^3.2.3",
    "husky": ">=6",
    "lint-staged": "12.1.7",
    "mrm": "^3.0.10",
    "prettier": ">=2",
    "typescript": "^4.5.4",
    "unocss": "0.22.3",
    "vite": "2.7.12",
    "vite-plugin-solid": "2.2.3"
  },
  "dependencies": {
    "@solid-primitives/storage": "1.1.0",
    "solid-app-router": "0.2.1",
    "solid-js": "1.3.3",
    "solid-meta": "0.27.3"
  },
  "lint-staged": {
    "*.{js,css,md,ts,yaml,json,tsx,html}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "ansi-regex@>2.1.1 <5.0.1": ">=5.0.1",
      "follow-redirects@<1.14.7": ">=1.14.7"
    }
  }
}
