{
  "name": "sobolid",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "VITE_HASHED_VERSION=`git rev-parse HEAD` vite build",
    "deploy": "VITE_HASHED_VERSION=`git rev-parse HEAD` VITE_PATHPREFIX=/sobolid/ vite build && gh-pages -d dist",
    "start": "vite preview",
    "format": "prettier --loglevel warn --write \"**/*.{js,css,md,ts,json,tsx,html}\"",
    "posttest": "npm run format",
    "lint2": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=6.0.0"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@unocss/preset-wind": "^0.27.2",
    "@unocss/reset": "^0.27.2",
    "eslint": "8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-solid": "^0.4.4",
    "fast-glob": "^3.2.11",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "rollup": "^2.68.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "^4.5.5",
    "unocss": "^0.27.2",
    "vite": "^2.8.4",
    "vite-plugin-solid": "^2.2.5"
  },
  "dependencies": {
    "@solid-primitives/storage": "1.1.1",
    "@supabase/supabase-js": "^1.30.6",
    "date-fns": "^2.28.0",
    "solid-app-router": "^0.3.0",
    "solid-js": "^1.3.8",
    "solid-meta": "^0.27.3",
    "solid-supabase": "^0.0.6"
  },
  "lint-staged": {
    "*.{js,css,md,ts,json,tsx,html}": "prettier --write"
  }
}
